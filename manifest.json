{
  "manifest_version": 2,
  "name": "Common sites",
  "description": "This is first extension",
  "version": "1.0.0",
  "icons": { "128": "icon_128.png" },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  // "permissions": ["activeTab"],

  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["/js/index.js"]
    }
  ]
}
